{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@svg/*": ["src/files/svg/*"],
      "@png/*": ["src/files/png/*"],
      "@styles/*": ["src/styles/*"]
    }
  }
}